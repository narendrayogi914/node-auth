{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node-server"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "server.js"
    },
    {
      "source": "/(.*)",
      "destination": "frontend/index.html"
    },
    {
      "source": "/",
      "destination": "frontend/index.html"
    }
  ]
}
